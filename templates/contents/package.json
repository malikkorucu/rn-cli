{
  "name": "lisamobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean:android": "cd android && gradlew clean && cd ..",
    "clean:ios": "cd ios && xcodebuild clean && cd ..",
    "release-aab:android": "cd android && gradlew bundleRelease && cd ..",
    "apk-macos": "cd android && ./gradlew assembleRelease  && cd ..",
    "aab-macos": "cd android && ./gradlew bundleRelease",
    "apk-windows": "cd android && gradlew assembleRelease  && cd ..",
    "android-release": "npx react-native run-android --variant=release",
    "cc": "npm start -- --reset-cache",
    "android": "npx react-native run-android",
    "ios": "npx react-native run-ios --simulator='iPhone'",
    "link": "npx react-native link",
    "splash": "npx react-native set-splash --path ./src/assets/SplashScreen.png",
    "icon": "npx react-native set-icon --path ./src/assets/logo.png",
    "start": "npx react-native start",
    "pod": "cd ios; pod install; cd ..",
    "svgr": "svgr ./src/assets/svg -d ./src/components/icons --native --svg-props className= --svgo-config .svgorc.json",
    "test": "jest",
    "codepush:android": "appcenter codepush release-react -a malik-4alabs.io/LittleLunches -d Production",
    "codepush:ios": "appcenter codepush release-react -a malik-4alabs.io/LittleLunches-1 -d Production",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
